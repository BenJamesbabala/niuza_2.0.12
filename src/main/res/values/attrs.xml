<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- TopBar -->
    <attr name="topbarTitle" format="reference|string" />
    <!-- title 对齐方式，靠左还是居中 -->
    <attr name="topbarTitleAlign" format="enum">
        <enum name="center" value="0"/>
        <enum name="left" value="1"/>
    </attr>
    <declare-styleable name="TopBar">
        <attr name="topbarTitle"/>
        <!-- title 对齐方式，靠左还是居中 -->
        <attr name="topbarTitleAlign" />
        <!-- 标题背景-->
        <attr name="titleBackground" format="reference" />
        <!-- 如果左钮是图片模式，必须设置此项 -->
        <attr name="leftButtonDrawable" format="reference" />
        <!-- 如果左按钮是文字模式，必须设置此项 -->
        <attr name="leftButtonText" format="reference|string" />
        <!-- 如果右按钮是图片模式，必须设置此项 -->
        <attr name="rightButtonDrawable" format="reference" />
        <!-- 如果右按钮是文字模式，必须设置此项 -->
        <attr name="rightButtonText" format="reference|string" />
    </declare-styleable>

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsDividerWidth" format="dimension" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
        <attr name="pstsPaddingMiddle" format="boolean" />
        <attr name="pstsTextAlpha" format="float" />
        <attr name="pstsTextSelectedAlpha" format="float" />

        <attr name="pstsTextStyle">
            <flag name="bold" value="0x00000001" />
            <flag name="italic" value="0x00000002" />
            <flag name="normal" value="0x00000000" />
        </attr>
        <attr name="pstsTextSelectedStyle">
            <flag name="bold" value="0x00000001" />
            <flag name="italic" value="0x00000002" />
            <flag name="normal" value="0x00000000" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MultipleSelectionButton">
        <attr name="ascv_unselectedTextColor" format="reference" />
        <attr name="ascv_unselectedColor" format="reference" />
        <attr name="ascv_selectedColor" format="reference" />
